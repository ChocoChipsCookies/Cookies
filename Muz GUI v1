-- Particles Script 

local function VAFBWEQ_script()
	local script = Instance.new('Script', parts)

	script.Parent.MouseButton1Click:Connect(function()
		for i, v in pairs(game.Players:GetChildren()) do
			emit = Instance.new("ParticleEmitter")
			emit.Parent = v.Character.Head
			emit.Texture = "http://www.roblox.com/asset/?id=120044006227669"
			emit.VelocitySpread = 100000
		end
		end)
end
coroutine.wrap(VAFBWEQ_script)()

-- Theme/Music

local function YRWDL_script()
	local script = Instance.new('Script', theme)

	function onClick()
		local theme		= Instance.new("Sound")
		theme.Parent		= game:GetService("Workspace")
		theme.SoundId		= "rbxassetid://ID" -- Place With Your Audio ID
		theme.Playing		= true
		theme.Looped		= false
		theme.Pitch      = 0.8
		theme.Volume		= 1
		theme.Distortion = 1999999999
	
	end
	
	script.Parent.MouseButton1Down:connect(onClick)
end
coroutine.wrap(YRWDL_script)()

-- Shutdown Script 

local function AZTAN_script()
	local script = Instance.new('Script', shut)

	script.Parent.MouseButton1Down:connect(function()
		for i, players in pairs(game.Players:GetChildren()) do
			players:Kick("GAME HAS BEEN HACKED BY TEAM MUZ, LFMAOOOO NIGGA")
		end
	end)
end
coroutine.wrap(AZTAN_script)()

