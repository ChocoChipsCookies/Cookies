		s.SkyboxDn = "http://www.roblox.com/asset/?id=120044006227669"
		s.SkyboxFt = "http://www.roblox.com/asset/?id=120044006227669"
		s.SkyboxLf = "http://www.roblox.com/asset/?id=120044006227669"
		s.SkyboxRt = "http://www.roblox.com/asset/?id=120044006227669"
		s.SkyboxUp = "http://www.roblox.com/asset/?id=120044006227669"
		game.Lighting.TimeOfDay = 12
	end
	
	script.Parent.MouseButton1Down:connect(click)
end
coroutine.wrap(VDIFZ_script)()
